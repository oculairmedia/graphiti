# Production Environment Configuration for Graphiti Frontend

# API Configuration
# When using nginx proxy, all connections go through port 8088
VITE_API_BASE_URL=/api
VITE_RUST_SERVER_URL=http://192.168.50.90:3000
VITE_WEBSOCKET_URL=/ws
# WebSocket URL is relative and will be constructed by the frontend code
# Override these to use specific WebSocket endpoints
# VITE_RUST_WS_URL=wss://your-domain.com/rust-ws
VITE_RUST_WS_PORT=3000

# Application Settings
VITE_APP_TITLE=Graphiti Knowledge Graph
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Performance Settings
VITE_MAX_GRAPH_NODES=10000
VITE_CACHE_SIZE=5000
VITE_WEBSOCKET_RECONNECT_ATTEMPTS=5

# Security Settings
VITE_ENABLE_CSP=true
VITE_ALLOWED_ORIGINS=http://localhost:8080,https://your-domain.com