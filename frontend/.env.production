# Production Environment Configuration for Graphiti Frontend

# API Configuration
VITE_API_BASE_URL=http://192.168.50.90:3000
VITE_WS_URL=ws://192.168.50.90:3000/ws

# Application Settings
VITE_APP_TITLE=Graphiti Knowledge Graph
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Performance Settings
VITE_MAX_GRAPH_NODES=10000
VITE_CACHE_SIZE=5000
VITE_WEBSOCKET_RECONNECT_ATTEMPTS=5

# Security Settings
VITE_ENABLE_CSP=true
VITE_ALLOWED_ORIGINS=http://localhost:8080,https://your-domain.com